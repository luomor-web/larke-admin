        "laravel",
        "lake",
        "larke",
        "admin",
        "larke-admin"
    ],
    "homepage": "https://github.com/luomor-web/larke-admin",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Peter Zhang",
            "email": "zhangchunsheng423@gmail.com",
            "homepage": "https://github.com/zhangchunsheng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "composer/semver": "^3.2",
        "phpseclib/phpseclib": "^3.0",
        "ramsey/uuid": "^4.3",
        "lake/larke-pclzip": "~1.0.0",
        "lake/larke-jwt": "~1.2.2",
        "lake/larke-auth": "~1.3.0"
